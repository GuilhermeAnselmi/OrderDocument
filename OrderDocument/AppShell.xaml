<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OrderDocument.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OrderDocument"
    xmlns:views="clr-namespace:OrderDocument.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:Folders}"
        Route="MainPage" />

    <ShellContent
        Title="Test"
        ContentTemplate="{DataTemplate views:Documents}"
        Route="Temp" />

</Shell>
